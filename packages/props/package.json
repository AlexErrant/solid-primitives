{
  "name": "@solid-primitives/props",
  "version": "2.2.0",
  "description": "Library of primitives focused around component props.",
  "author": "Alex Lohr <alex.lohr@logmein.com>",
  "contributors": [
    "Damian Tarnawski <gthetarnav@gmail.com>"
  ],
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/props",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "props",
    "stage": 3,
    "list": [
      "combineProps",
      "omitProps",
      "pickProps",
      "createControlledProps"
    ],
    "category": "Utilities"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "vite serve dev --host",
    "dev": "npm run start",
    "build": "tsup",
    "test": "uvu -r solid-register"
  },
  "keywords": [
    "props",
    "knobs",
    "controls",
    "solid",
    "primitives"
  ],
  "dependencies": {
    "@solid-primitives/utils": "^2.0.0"
  },
  "devDependencies": {
    "esbuild-plugin-solid": "^0.4.2",
    "jsdom": "^19.0.0",
    "nanospy": "^0.5.0",
    "prettier": "^2.5.1",
    "solid-register": "^0.2.5",
    "tslib": "^2.4.0",
    "tsup": "^5.12.7",
    "typescript": "^4.6.4",
    "uvu": "^0.5.3",
    "unocss": "0.31.13",
    "vite": "2.9.5",
    "vite-plugin-solid": "2.2.6"
  },
  "peerDependencies": {
    "solid-js": "^1.3.0"
  }
}
