{
  "name": "@solid-primitives/rootless",
  "version": "0.1.0",
  "description": "A collection of primitives that help dealing with Solid's reactive roots.",
  "author": "Damian Tarnawski @thetarnav <gthetarnav@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/davedbase/solid-primitives/tree/main/packages/rootless#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davedbase/solid-primitives.git"
  },
  "primitive": {
    "name": "rootless",
    "stage": 1,
    "list": [
      "createSubRoot",
      "createCallback",
      "runWithRoot",
      "runWithSubRoot",
      "createSharedRoot"
    ],
    "category": "Reactivity"
  },
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "uvu -r solid-register"
  },
  "keywords": [
    "solid",
    "primitives"
  ],
  "dependencies": {
    "@solid-primitives/utils": "^0.2.1"
  },
  "devDependencies": {
    "jsdom": "18.1.1",
    "prettier": "^2.5.1",
    "solid-register": "0.1.5",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "tsup": "^5.11.11",
    "uvu": "^0.5.2"
  },
  "peerDependencies": {
    "solid-js": "^1.3.1"
  }
}