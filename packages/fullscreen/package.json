{
  "name": "@solid-primitives/fullscreen",
  "version": "1.0.6",
  "description": "Primitive that wraps the fullscreen API.",
  "author": "Alex Lohr <alex.lohr@logmein.com>",
  "license": "MIT",
  "homepage": "https://github.com/davedbase/solid-primitives/tree/main/packages/fetch",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davedbase/solid-primitives.git"
  },
  "primitive": {
    "name": "fullscreen",
    "stage": 3,
    "list": [
      "createFullscreen"
    ],
    "category": "Browser APIs"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/server.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "node": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "clean": "rimraf dist/",
    "build": "tsup",
    "test": "echo disabled defunct jest test suite"
  },
  "keywords": [
    "fullscreen",
    "display",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "@babel/preset-env": "7.15.6",
    "@types/jest": "27.0.1",
    "babel-preset-solid": "1.1.5",
    "jest": "27.0.1",
    "jsdom": "^18.1.1",
    "prettier": "^2.0.5",
    "ts-jest": "27.0.5",
    "tslib": "^2.0.1",
    "tsup": "^5.10.1",
    "typescript": "^4.0.2"
  },
  "peerDependencies": {
    "solid-js": "^1.3.0"
  },
  "jest": {
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json",
        "babelConfig": {
          "presets": [
            "babel-preset-solid",
            "@babel/preset-env"
          ]
        }
      }
    },
    "setupFiles": [
      "./test/setup.ts"
    ],
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "solid-js/web": "<rootDir>/node_modules/solid-js/web/dist/web.cjs",
      "solid-js": "<rootDir>/node_modules/solid-js/dist/solid.cjs"
    }
  }
}
