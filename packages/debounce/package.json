{
  "name": "@solid-primitives/debounce",
  "version": "1.2.0",
  "description": "Primitive that creates a debounce function",
  "author": "David Di Biase <dave.dibiase@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/davedbase/solid-primitives/tree/main/packages/debounce",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davedbase/solid-primitives.git"
  },
  "primitive": {
    "name": "debounce",
    "stage": 3,
    "list": [
      "createDebounce"
    ],
    "category": "Utilities"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "node": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "start": "vite -r ./dev/ -c ./dev/vite.config.ts",
    "build": "tsup",
    "test": "uvu -r solid-register"
  },
  "keywords": [
    "debounce",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "prettier": "^2.5.1",
    "solid-register": "^0.1.5",
    "tslib": "^2.3.1",
    "tsup": "^5.12.0",
    "typescript": "^4.6.2",
    "uvu": "^0.5.3"
  },
  "peerDependencies": {
    "solid-js": ">=1.0.0"
  },
  "solid-register": {
    "dom": false
  }
}
